{
    "name": "nostur/nostrmedia",
    "description": "Host the images or media you post to nostr on your own WordPress installation",
    "autoload": {
        "psr-4": {
            "Nostur\\NostrMedia\\": "/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/fabianfabian/nostr-php"
        }
    ],
    "require": {
        "swentel/nostr-php": "dev-verify-method"
    }
}
